<%- include ('./layouts/header') -%>
    <div class="container mt-5">
        <h3 style="text-align:center" class="my-4">REGISTRO DE USUARIO</h3>
        <form method="POST" action="/access/register" autocomplete="off">
            <div class="input-group mb-3">
                <input type="text" class="form-control" id="username" placeholder="Usuario"
                    aria-label="Recipient's username" aria-describedby="button-addon2" name="username" required>
                <input type="text" class="form-control" id="password" placeholder="ContraseÃ±a"
                    aria-label="Recipient's username" aria-describedby="button-addon2" name="password" required>

                <div class="input-group-append">
                    <button class="btn btn-outline-secondary" type="submit" id="button-addon2">Registrarse</button>
                </div>
            </div>
        </form>
    </div>
    <div class="container">
        <div class="input-group-append">
            <a href="/">
                <button class="btn btn-outline-secondary" type="submit" id="button-addon2">Volver</button>
            </a>
        </div>

    </div>
    <%- include ('./layouts/footer') -%>